{
  "inputStream": {
    "type": "LiveStream",
    "constraints": {
      "width": {
        "min": 450
      },
      "height": {
        "min": 300
      },
      "facingMode": "environment",
      "aspectRatio": {
        "min": 1,
        "max": 2
      }
    }
  },
  "locate": true,
  "numOfWorkers": 0,
  "frequency":  20,
  "decoder": {
    "drawBoundingBox": true,
    "drawScanline": true,
    "showPattern": true,
    "readers": [
      "ean_reader",
      "ean_8_reader"
    ]
  },
  "locator": {
    "patchSize": "medium",
    "halfSample": true
  },
  "area": { 
    "top": "20%",  
    "right": "10%", 
    "left": "10%",  
    "bottom": "20%"
  }
}